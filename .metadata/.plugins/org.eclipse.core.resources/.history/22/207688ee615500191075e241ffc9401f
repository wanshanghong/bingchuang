<!-- 写Sql语句  patientId;
	private double temp;
	private Date date; -->
<mapper namespace="cn.bingchuang.mapper.PatientMapper">
	<insert id="addtemperature"  parameterType="cn.bingchuang.pojo.Temperature">
		insert into temperature(patient_id,temp,date)
		values (#{patientId},#{temp},#{date})
	</insert>
</mapper>